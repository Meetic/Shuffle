<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.meetic.shuffle.sample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".HorizontalActivity"/>
        <activity android:name=".HorizontalInlineActivity"/>
        <activity android:name=".HorizontalInlineBehindActivity"/>
        <activity android:name=".HorizontalInlineWithoutRotationActivity"/>
        <activity android:name=".VerticalActivity"/>
        <activity android:name=".EnableDisableActivity"/>
        <activity android:name=".StackChangeActivity"/>
        <activity android:name=".HorizontalInlineWrapContentActivity"/>
        <activity android:name=".RestartActivity"/>
    </application>

</manifest>